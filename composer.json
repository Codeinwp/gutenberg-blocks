{
  "name": "codeinwp/gutenberg-blocks",
  "description": "A set of awesome Gutenberg Blocks!",
  "type": "library",
  "version": "1.6.0",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/Codeinwp/gutenberg-blocks",
  "authors": [
    {
      "name": "ThemeIsle team",
      "email": "friends@themeisle.com",
      "homepage": "https://themeisle.com"
    }
  ],
  "support": {
    "issues": "https://github.com/Codeinwp/gutenberg-blocks/issues"
  },
  "require": {
    "tubalmartin/cssmin": "^4.1",
    "masterminds/html5": "^2.7"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3",
    "wp-coding-standards/wpcs": "^1",
    "phpcompatibility/php-compatibility": "^9",
    "dealerdirect/phpcodesniffer-composer-installer": "0.7.1",
    "automattic/vipwpcs": "^0.3.0",
    "phpunit/phpunit": "^5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "5.6"
    }
  },
  "extra": {
    "installer-disable": "true"
  },
  "scripts": {
    "lint": "phpcs",
    "format": "phpcbf"
  }
}
